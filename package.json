{
  "name": "@traced-fabric/core",
  "type": "module",
  "version": "0.10.0",
  "description": "A TypeScript-compatible JavaScript library for tracking and synchronizing value states across different environments.",
  "author": "Andrey Kovyarov <andrey.kovyarov@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/traced-fabric/core/wiki",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/traced-fabric/core.git"
  },
  "bugs": {
    "url": "https://github.com/traced-fabric/core/issues",
    "email": "andrey.kovyarov@gmail.com"
  },
  "keywords": [
    "javascript",
    "typescript",
    "traced-fabric-core",
    "value-tracking",
    "state-sync",
    "data-proxy",
    "trace-sync",
    "websockets-data",
    "traceable-objects",
    "data-mirroring",
    "complex-data-tracking",
    "value-state-management"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "index.ts",
    "src/**/*"
  ],
  "scripts": {
    "lint": "eslint --cache . && tsc --noEmit",
    "test": "bun test",
    "coverage": "bun test --coverage",
    "build": "bunx --bun vite build",
    "prepare": "husky",
    "release": "bun run build && changeset publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.27.1",
    "@changesets/cli": "^2.27.7",
    "@types/bun": "^1.1.8",
    "ajv": "^8.17.1",
    "eslint": "^9.9.1",
    "eslint-plugin-unused-imports": "^4.1.3",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.9",
    "typescript": "5.5.4",
    "vite": "^5.4.2",
    "vite-plugin-dts": "^4.0.3"
  },
  "lint-staged": {
    "*": [
      "eslint --cache --fix"
    ]
  }
}
