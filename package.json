{
  "name": "@traced-fabric/core",
  "type": "module",
  "version": "0.3.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/traced-fabric/core.git"
  },
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "index.ts",
    "src/**/*"
  ],
  "scripts": {
    "lint": "eslint --cache . && tsc --noEmit",
    "test": "bun test",
    "coverage": "bun test --coverage",
    "build": "bunx --bun vite build",
    "prepare": "husky",
    "release": "bun run build && changeset publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.27.1",
    "@changesets/cli": "^2.27.7",
    "@types/bun": "^1.1.7",
    "ajv": "^8.17.1",
    "eslint": "^9.9.1",
    "eslint-plugin-unused-imports": "^4.1.3",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.9",
    "typescript": "5.4.2",
    "vite": "^5.4.2",
    "vite-plugin-dts": "^4.0.3"
  },
  "lint-staged": {
    "*": [
      "eslint --cache --fix"
    ]
  }
}
